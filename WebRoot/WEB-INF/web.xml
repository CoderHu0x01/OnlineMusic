<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>OnlineMusic</display-name>
  <servlet>
    <servlet-name>BaseServlet</servlet-name>
    <servlet-class>com.scme.onlinemusic.utils.BaseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.scme.onlinemusic.user.web.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MusicServlet</servlet-name>
    <servlet-class>com.scme.onlinemusic.music.web.servlet.MusicServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MusicBoxServlet</servlet-name>
    <servlet-class>com.scme.onlinemusic.musicbox.web.servlet.MusicBoxServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InfoServlet</servlet-name>
    <servlet-class>com.scme.onlinemusic.info.web.servlet.InfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BaseServlet</servlet-name>
    <url-pattern>/BaseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MusicServlet</servlet-name>
    <url-pattern>/MusicServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MusicBoxServlet</servlet-name>
    <url-pattern>/MusicBoxServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InfoServlet</servlet-name>
    <url-pattern>/InfoServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>